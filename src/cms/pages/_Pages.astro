---
import type { ContentPayload } from "../../graphql/shared/ContentPayload";
import BlogPostPage from "./BlogPostPage/BlogPostPage.astro";
import LandingPage from "./LandingPage/LandingPage.astro";
const contentPayload = Astro.props.data as ContentPayload;
const pageType = contentPayload.types[0];
---
{pageType === "BlogPostPage" &&
    <BlogPostPage contentPayload={contentPayload}/>
}
{pageType === "LandingPage" &&
    <LandingPage contentPayload={contentPayload}/>
}
